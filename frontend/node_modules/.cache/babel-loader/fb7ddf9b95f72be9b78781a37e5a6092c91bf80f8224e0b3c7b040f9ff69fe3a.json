           console.log(\"Editing article:\", article);\n        },\n        deleteArticle(articleId) {\n            // Implement deletion logic here\n            console.log(\"Deleting article with ID:\", articleId);\n        },\n        clearForm() {\n            this.title = \"\";\n        },\n    },\n};\n</script>\n\n<style scoped>\n/* Add your custom styles here */\n</style>\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,CAAA,MAAO,QAAQ,EAAE;AACxB,OAAO,sCAAsC,EAAE;AAC/C,OAAO,gBAAgB,EAAE;;AAEzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MAETC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,YAAY,CAAC,CAAC;IACnB;IACAL,CAAC,CAAC,IAAI,CAACM,GAAG,CAAC,CAACC,IAAI,CAAC,OAAO,CAAC,CAACC,SAAS,CAAC,CAAC;EACzC,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACf,IAAI;QACA,MAAMX,KAAK,CAACY,IAAI,CAAC,6BAA6B,EAAE;UAC5CT,KAAK,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QACF,IAAI,CAACG,YAAY,CAAC,CAAC;QACnB,IAAI,CAACO,SAAS,CAAC,CAAC;MACpB,EAAE,OAAOC,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MACjD;IACJ,CAAC;IACD,MAAMR,YAAYA,CAAA,EAAG;MACjB,IAAI;QACA,MAAMU,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,iBAAiB,CAAC;QACnD,IAAI,CAACb,QAAO,GAAIY,QAAQ,CAACd,IAAI;QAC7B;QACAD,CAAC,CAAC,IAAI,CAACM,GAAG,CAAC,CAACC,IAAI,CAAC,OAAO,CAAC,CAACC,SAAS,CAAC,CAAC,CAACS,OAAO,CAAC,CAAC;QAC/CjB,CAAC,CAAC,IAAI,CAACM,GAAG,CAAC,CAACC,IAAI,CAAC,OAAO,CAAC,CAACC,SAAS,CAAC,CAAC;MACzC,EAAE,OAAOK,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACnD;IACJ,CAAC;IACDK,WAAWA,CAACC,OAAO,EAAE;MACjB;MACAL,OAAO,CAACM,GAAG,CAAC,kBAAkB,EAAED,OAAO,CAAC;IAC5C,CAAC;IACDE,aAAaA,CAACC,SAAS,EAAE;MACrB;MACAR,OAAO,CAACM,GAAG,CAAC,2BAA2B,EAAEE,SAAS,CAAC;IACvD,CAAC;IACDV,SAASA,CAAA,EAAG;MACR,IAAI,CAACV,KAAI,GAAI,EAAE;IACnB;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}