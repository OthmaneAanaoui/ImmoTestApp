{"ast":null,"code":"import axios from \"axios\";\nimport $ from \"jquery\"; // Import jQuery\nimport \"bootstrap/dist/css/bootstrap.min.css\"; // Import Bootstrap CSS\nimport \"datatables.net\"; // Import DataTables\n\nexport default {\n  data() {\n    return {\n      title: \"\",\n      articles: []\n    };\n  },\n  mounted() {\n    this.loadArticles();\n    // Initialize DataTables plugin\n    $(this.$el).find(\"table\").DataTable();\n  },\n  methods: {\n    async addArticle() {\n      try {\n        await axios.post(\"localhost:8000/api/articles\", {\n          title: this.title\n        });\n        this.loadArticles();\n        this.clearForm();\n      } catch (error) {\n        console.error(\"Error adding article:\", error);\n      }\n    },\n    async loadArticles() {\n      try {\n        const response = await axios.get(\"../api/articles\");\n        this.articles = response.data;\n        // Destroy and reinitialize DataTables to reflect changes\n        $(this.$el).find(\"table\").DataTable().destroy();\n        $(this.$el).find(\"table\").DataTable();\n      } catch (error) {\n        console.error(\"Error loading articles:\", error);\n      }\n    },\n    editArticle(article) {\n      // Implement editing logic here\n      console.log(\"Editing article:\", article);\n    },\n    deleteArticle(articleId) {\n      // Implement deletion logic here\n      console.log(\"Deleting article with ID:\", articleId);\n    },\n    clearForm() {\n      this.title = \"\";\n    }\n  }\n};","map":{"version":3,"names":["axios","$","data","title","articles","mounted","loadArticles","$el","find","DataTable","methods","addArticle","post","clearForm","error","console","response","get","destroy","editArticle","article","log","deleteArticle","articleId"],"sources":["/Users/aanaouiothmane/Desktop/ImmoTestApp/frontend/src/components/AddArticle.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <h2>Ajouter un article</h2>\n        <form @submit.prevent=\